{
  "private": true,
  "scripts": {
    "build": "pnpm turbo run build",
    "build:gh-page-fe": "pnpm turbo run build --filter gh-page-fe",
    "build:gh-page-be": "pnpm turbo run build --filter gh-page-be",
    "dev": "pnpm turbo run dev",
    "dev:gh-page-fe": "pnpm turbo run dev --filter gh-page-fe",
    "dev:gh-page-be": "pnpm turbo run dev --filter gh-page-be",
    "lint": "pnpm turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "8.45.0",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "node-monorepo"
}
