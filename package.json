{
  "name": "jayur830.github.io",
  "private": true,
  "packageManager": "pnpm@7.15.0",
  "scripts": {
    "build": "pnpm turbo run build",
    "build:fe": "pnpm turbo run build --filter fe",
    "build:be": "pnpm turbo run build --filter be",
    "dev": "pnpm turbo run dev",
    "dev:fe": "pnpm turbo run dev --filter fe",
    "dev:be": "pnpm turbo run dev --filter be",
    "lint": "pnpm turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "codegen": "pnpm turbo run codegen --filter fe"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@turbo/gen": "^1.10.12",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.46.0",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "turbo": "latest"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
