{
  "root": true,
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": ["next", "prettier"],
  "plugins": ["simple-import-sort", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "simple-import-sort/imports": "warn",
    "simple-import-sort/exports": "warn"
  }
}
