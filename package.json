{
  "name": "jayur830.github.io",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "setup": "yarn dlx @yarnpkg/sdks vscode",
    "postinstall": "yarn setup",
    "dev": "next dev",
    "build": "rm -rf ./.next && rm -rf ./docs && next build && touch ./docs/.nojekyll",
    "start": "next start",
    "preview": "http-server ./docs -p 8080",
    "lint": "next lint --fix . --ext .tsx,.ts && prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|scss|md)\"",
    "codegen": "graphql-codegen && next lint --fix --quiet --file ./src/graphql/graphql.ts"
  },
  "dependenciesMeta": {
    "next@13.3.0": {
      "unplugged": true
    }
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.130",
    "@mui/material": "^5.13.1",
    "dayjs": "^1.11.7",
    "encoding": "^0.1.13",
    "firebase": "^10.0.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "immer": "^10.0.2",
    "lodash": "^4.17.21",
    "next": "13.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.7",
    "supports-color": "8.1.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@svgr/webpack": "^8.0.1",
    "@types/firebase": "^3.2.1",
    "@types/lodash": "^4",
    "@types/node": "20.2.3",
    "@types/prettier": "^2",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "simple-git-hooks": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "!(*test).{ts,tsx}": [
      "next lint --fix . --ext .tsx,.ts",
      "prettier --write \"**/*.+(js|jsx|ts|tsx)\""
    ],
    "*.{json,md,html,scss}": [
      "prettier --write \"**/*.+(json|yml|yaml|css|scss|md)\""
    ]
  }
}
